{
  "ConnectionStrings": {
    "Redis": "#{redis.host}:#{redis.port},password=#{redis.password}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Slack": {
    "OauthToken": "#{slack.oauthtoken}",
    "SigningSecret": "#{slack.signingsecret}",
    "VerificationToken": "#{slack.verificationtoken}"
  },
  "BingSearch": {
    "Key": "#{bingsearch.key}"
  }
}
